<% include ../partials/header %>
<% include ../partials/sheetjs %>
<div class="jumbotron" id="drop">
	<h1 style="text-align: center">Drag and Drop Uploader</h1>
	<br>
	<h5 style="text-align: center">Drop a spreadsheet file here to change table data</H3>
</div>
<div class="justify-content-md-center">
	<pre>
<table id="datatable" class="no-more-tables">
  <tr><td contenteditable="true" colspan="4">Imported File Template</td></tr>
  <tr><td contenteditable="true">This</td><td contenteditable="true">is</td><td contenteditable="true">a</td><td contenteditable="true">Template</td></tr>
  <tr><td contenteditable="true">It</td><td contenteditable="true">will</td><td contenteditable="true">be</td><td contenteditable="true">replaced</td></tr>
</table>
</pre>
</div>
<br />
<div class="container">
	<div class="row justify-content-md-center">
		<div class="form-goup" id="xportxlsx">
			<input class="btn btn-outline-secondary" type="submit" name="submit" value="Export to XLSX!"
				onclick="doit('xlsx');">
		</div>
		<p id="xlsxbtn" class="btn">Flash required for actually downloading the generated file.</p>
		<div class="form-goup" id="xportcsv">
			<input class="btn btn-outline-secondary" type="submit" name="submit" value="Export to XLSB!"
				onclick="doit('xlsb');">
		</div>
		<p id="xlsbbtn" class="btn">Flash required for actually downloading the generated file.</p>
		<div class="form-goup" id="xportcsv">
			<input class="btn btn-outline-secondary" type="submit" name="submit" value="Export to CSV!"
				onclick="doit('csv');">
		</div>
		<p id="csvbtn" class="btn">Flash required for actually downloading the generated file.</p>
	</div>
</div>
<% include ../partials/scripts %>
<script type="text/javascript" src="/scripts/xlsx-util.js"></script>
<% include ../partials/footer %>