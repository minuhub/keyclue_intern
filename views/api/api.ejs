<% include ../partials/header %>
<div class="container">
    <div class="jumbotron">
        <h1 style="text-align: center">상품관리</h1>
    </div>
    <div style="width: 30%; margin: 25px auto;">
        <form action="/api" method="post">
            <div class="form-group">
                <input class="form-control" type="text" name="username" placeholder="username">
            </div>
            <div class="form-group">
                <input class="form-control" type="password" name="password" placeholder="password">
            </div>
            <br>
            <div class="form-goup">
                <input class="btn btn-outline-secondary btn-block" type="submit" name="submit" value="상품보기">
            </div>
            <br>
        </form>
        <br>
        <a href="/api/xlsx"><button class="btn btn-outline-secondary btn-block">엑셀 업로드</button></a>
        <br>
        <a href="/api/modify"><button class="btn btn-outline-secondary btn-block">엑셀수정</button></a>
        <br>
        <a href="/api/ajax"><button class="btn btn-outline-secondary btn-block">AJAX</button></a>
        <br>
        <a href="/api"><button class="btn btn-outline-secondary btn-block">Go Back</button></a>
    </div>
</div>
<% include ../partials/scripts %>
<script type="text/javascript" src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>
<link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css" />
<script>
    $('input[name="dates"]').daterangepicker({
opens: 'center'
}, function(start, end, label) {
console.log("A new date selection was made: " + start.format('YYYY-MM-DD') + ' to ' + end.format('YYYY-MM-DD'));
});
</script>
